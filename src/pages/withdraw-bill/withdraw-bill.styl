page
    height 100%
.wrap
    min-height 100%
    padding-bottom 30rpx
.filterImg
    width 34rpx
    height 34rpx
#timeFilter 
    height 120rpx
   
.filterMask
    position fixed
    background rgba(0, 0, 0, 0.5)
    left 0
    top 0
    width 100%
    height 100%
.filterMain
    background #fff
    padding-top 100rpx
.filterTab
    width 200rpx
    height 70rpx
    background #f7f7f7
    border-radius 6rpx
    color #333
    font-size 28rpx
    line-height 70rpx
    text-align center
.filterTabRed
    background #FFE9E9
    color #E93421
.filterBtnWrap
    margin-top 170rpx
.filterBtnWrap view
    width 50%
    height 90rpx
    line-height 90rpx
    text-align center
    color #fff
    font-size 34rpx
    background #E93421
#reset
    background #FFE9E9
    color #E93421
.fillTitle
    position fixed
    left 0
    top 0
    width 100%
    box-sizing border-box
.arrow_wrap
    height 12rpx
    overflow hidden
    margin-left 10rpx
.arrow
    border-width 12rpx
    border-style solid
    border-color #333 transparent transparent transparent
.listTitle
    height 100rpx
.listTitle
    font-size 28rpx
    color #999
.list
    padding 236rpx 30rpx 0
.item
    height 132rpx
.bill_icon
    padding-top 5rpx
    width 40rpx
    height 40rpx


